# Required with Godaddy
Options -MultiViews

# Turn on mod_rewrite
RewriteEngine On

# Remove .html from URL
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]

# Remove index.html from URL
RewriteRule ^index\.html$ / [R=301,L]
